        <section name="Test Automation Project Configuration">
            <subsection name="Maven - Pom.xml">
                <strong>maven-compiler-plugin</strong>
                <blockquote>Responsibility : Compiles Java Code</blockquote>
                <blockquote>Responsibility : Controls Java Runtime Versioning</blockquote>

                <strong>maven-surefire-plugin</strong>
                <blockquote>Responsibility : Test Execution Management</blockquote>

                <strong>maven-surefire-report-plugin</strong>
                <blockquote>Responsibility : Surefire HTML Report addition to the site</blockquote>

                <strong>maven-site-plugin</strong>
                <blockquote>Responsibility : Maven Base HTML site</blockquote>

                <strong>copy-rename-maven-plugin</strong>
                <blockquote>Responsibility : Site Style deployment</blockquote>

                <strong>maven-javadoc-plugin</strong>
                <blockquote>Responsibility : Generated Code Documentation</blockquote>

                <strong>org.seleniumhq.selenium</strong>
                <blockquote>Responsibility : Selenium Web Driver Library</blockquote>

                <strong>cucumber-junit</strong>
                <blockquote>Responsibility : BDD Technology integration to JUnit</blockquote>

                <strong>cucumber-java</strong>
                <blockquote>Responsibility : BDD Technology Java Library</blockquote>

                <strong>org.apache.httpcomponents</strong>
                <blockquote>Responsibility : HTTP Communication Library</blockquote>

                <strong>org.apache.logging.log4j</strong>
                <blockquote>Responsibility : Log Management Framework</blockquote>

                <strong>com.google.code.gson</strong>
